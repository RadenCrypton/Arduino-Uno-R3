  digitalWrite(r, LOW);    // turn the LED off by making the voltage LOW
  digitalWrite(ledAirplane, LOW);    // turn the LED off by making the voltage LOW
  delay(60);              // wait for a second
  cekL();   // turn the LED on (HIGH is the voltage level)
