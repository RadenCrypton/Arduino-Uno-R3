  digitalWrite(l, LOW);    // turn the LED off by making the voltage LOW
  digitalWrite(ledAirplane, LOW);    // turn the LED off by making the voltage LOW
  delay(60);              // wait for a second
  
  }


void cekAirplane()
{
sein = digitalRead(sw1);
sUtama = digitalRead(sw2);
  if (sUtama ==LOW)
     { digitalWrite(ledAirplane, LOW); resetF();}
  else if (sUtama==HIGH && sein==LOW)
     { digitalWrite(ledAirplane, HIGH); }
  else 
     { digitalWrite(ledAirplane, LOW); }
}


void cekR()
{
sUtama = digitalRead(sw2); 
if ( sUtama==HIGH )
{digitalWrite(r, HIGH);}
else
{digitalWrite(r, LOW);}
}

void cekL()
{
sUtama = digitalRead(sw2); 
if ( sUtama==HIGH )
{digitalWrite(l, HIGH);}
else
{digitalWrite(l, LOW);}
}
