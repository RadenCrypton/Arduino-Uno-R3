#include 
#include 
#define SS_PIN 10
#define RST_PIN 5

RFID rfid(SS_PIN,RST_PIN);

void setup()
{
 Serial.begin(9600);
 SPI.begin();
 rfid.init();
}

void loop()
{ 
 if(rfid.isCard())
 {
  if(rfid.readCardSerial()) 
  { 
   String serial;
   for (int i=0; i<= 4; i++) 
   { 
    serial += String(rfid.serNum[i],DEC);
   } 
   Serial.println("Card ID: " + serial); 
     }
  
 }
 rfid.halt();
 delay(1000);
 
}

// http://www.rustamaji.net/id/arduino/proyek-rfid-1-membaca-id
